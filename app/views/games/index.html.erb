<div class="cards">
  <% @games.each do |game| %>
    <div class="card-container ">
      <%= link_to image_tag("https://source.unsplash.com/random/", alt: "random image"), game_path(game) %>
      <div class="card-game-info">
        <h1><%= game.name %></h1>
      </div>
    </div>
  <% end %>
</div>
